{
    "exclude":["node_modules"],
    "include": ["./src/**/*.tsx","./src/**/*.ts"],
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "noImplicitUseStrict": true,
        "noImplicitAny": false,
        "alwaysStrict": false,
        "sourceMap": true,
        "allowJs": true,
        "module": "CommonJS",
        "target": "es2017",
        "baseUrl": ".",
        "jsx": "react",
        "paths":{
            "global-status":["./src/globalStatus"],
            "component":["./src/component"],
            "pages":["./src/pages"],
            "entry":["./src/entry"],
            "utils":["./src/utils"],
        },
    },
}